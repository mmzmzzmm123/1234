<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.student.mapper.StuInfoMapper">

    <resultMap type="StuInfo" id="StuInfoResult">
        <result property="id"    column="id"    />
        <result property="studentId"    column="student_id"    />
        <result property="studentName"    column="student_name"    />
        <result property="idNum"    column="id_num"    />
        <result property="deptId"    column="dept_id"    />
        <result property="deptName"    column="dept_name"    />
        <result property="trainingLevel"    column="training_level"    />
        <result property="grade"    column="grade"    />
        <result property="address"    column="address"    />
        <result property="nation"    column="nation"    />
        <result property="campus"    column="campus"    />
        <result property="studentTag"    column="student_tag"    />
        <result property="accommodation"    column="accommodation"    />
        <result property="accommodationPark"    column="accommodation_park"    />
        <result property="dormitoryNo"    column="dormitory_no"    />
        <result property="isOnSchool"    column="is_on_school"    />
        <result property="controlLevel"    column="control_level"    />
        <result property="notSchoolReason"    column="not_school_reason"    />
        <result property="placeToSchoolLevel"    column="place_to_school_level"    />
        <result property="placeToSchool"    column="place_to_school"    />
        <result property="placeToLeave"    column="place_to_leave"    />
        <result property="jointType"    column="joint_type"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectStuInfoVo">
        select
            t.id,
            t.student_id,
            t.student_name,
            t.id_num,
            t.dept_id,
            u.dept_name,
            t.training_level,
            t.grade,
            t.address,
            t.nation, t.campus,
            t.student_tag,
            t.accommodation, t.accommodation_park, t.dormitory_no,
            t.is_on_school, control_level, not_school_reason, place_to_school_level, place_to_school,place_to_leave,
            t.joint_type,
            t.create_by, t.create_time, t.update_by, t.update_time, t.remark
        from stu_info t
        left join sys_dept u on t.dept_id = u.dept_id
    </sql>

    <select id="selectStuInfoList" parameterType="StuInfo" resultMap="StuInfoResult">
        <include refid="selectStuInfoVo"/>
        <where>
            1=1
            <if test="studentId != null "> and student_id = #{studentId}</if>
            <if test="studentName != null  and studentName != ''"> and student_name like concat('%', #{studentName}, '%')</if>
            <if test="idNum != null  and idNum != ''"> and id_num = #{idNum}</if>
            <if test="deptId != null "> and dept_id = #{deptId}</if>
            <if test="deptName != null  and deptName != ''"> and dept_name like concat('%', #{deptName}, '%')</if>
            <if test="trainingLevel != null  and trainingLevel != ''"> and training_level = #{trainingLevel}</if>
            <if test="grade != null  and grade != ''"> and grade = #{grade}</if>
            <if test="address != null  and address != ''"> and address = #{address}</if>
            <if test="nation != null  and nation != ''"> and nation = #{nation}</if>
            <if test="campus != null  and campus != ''"> and campus = #{campus}</if>
            <if test="studentTag != null  and studentTag != ''"> and student_tag = #{studentTag}</if>
            <if test="accommodation != null  and accommodation != ''"> and accommodation = #{accommodation}</if>
            <if test="accommodationPark != null  and accommodationPark != ''"> and accommodation_park = #{accommodationPark}</if>
            <if test="dormitoryNo != null  and dormitoryNo != ''"> and dormitory_no = #{dormitoryNo}</if>
            <if test="isOnSchool != null  and isOnSchool != ''"> and is_on_school = #{isOnSchool}</if>
            <if test="controlLevel != null  and controlLevel != ''"> and control_level = #{controlLevel}</if>
            <if test="notSchoolReason != null  and notSchoolReason != ''"> and not_school_reason = #{notSchoolReason}</if>
            <if test="placeToSchoolLevel != null  and placeToSchoolLevel != ''"> and place_to_school_level = #{placeToSchoolLevel}</if>
            <if test="placeToSchool != null  and placeToSchool != ''"> and place_to_school = #{placeToSchool}</if>
            <if test="placeToLeave != null  and placeToLeave != ''"> and place_to_leave = #{placeToLeave}</if>
            <if test="jointType != null  and jointType != ''"> and joint_type = #{jointType}</if>
        </where>
        ${params.dataScope}
    </select>
    
    <select id="selectStuInfoById" parameterType="Long" resultMap="StuInfoResult">
        <include refid="selectStuInfoVo"/>
        where id = #{id}
    </select>

    <insert id="insertStuInfo" parameterType="StuInfo" useGeneratedKeys="true" keyProperty="id">
        insert into stu_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="studentId != null">student_id,</if>
            <if test="studentName != null and studentName != ''">student_name,</if>
            <if test="idNum != null">id_num,</if>
            <if test="deptId != null">dept_id,</if>
            <if test="deptName != null">dept_name,</if>
            <if test="trainingLevel != null">training_level,</if>
            <if test="grade != null">grade,</if>
            <if test="address != null">address,</if>
            <if test="nation != null">nation,</if>
            <if test="campus != null">campus,</if>
            <if test="studentTag != null">student_tag,</if>
            <if test="accommodation != null">accommodation,</if>
            <if test="accommodationPark != null">accommodation_park,</if>
            <if test="dormitoryNo != null">dormitory_no,</if>
            <if test="isOnSchool != null">is_on_school,</if>
            <if test="controlLevel != null">control_level,</if>
            <if test="notSchoolReason != null">not_school_reason,</if>
            <if test="placeToSchoolLevel != null">place_to_school_level,</if>
            <if test="placeToSchool != null">place_to_school,</if>
            <if test="placeToLeave != null">place_to_leave,</if>
            <if test="jointType != null">joint_type,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="studentId != null">#{studentId},</if>
            <if test="studentName != null and studentName != ''">#{studentName},</if>
            <if test="idNum != null">#{idNum},</if>
            <if test="deptId != null">#{deptId},</if>
            <if test="deptName != null">#{deptName},</if>
            <if test="trainingLevel != null">#{trainingLevel},</if>
            <if test="grade != null">#{grade},</if>
            <if test="address != null">#{address},</if>
            <if test="nation != null">#{nation},</if>
            <if test="campus != null">#{campus},</if>
            <if test="studentTag != null">#{studentTag},</if>
            <if test="accommodation != null">#{accommodation},</if>
            <if test="accommodationPark != null">#{accommodationPark},</if>
            <if test="dormitoryNo != null">#{dormitoryNo},</if>
            <if test="isOnSchool != null">#{isOnSchool},</if>
            <if test="controlLevel != null">#{controlLevel},</if>
            <if test="notSchoolReason != null">#{notSchoolReason},</if>
            <if test="placeToSchoolLevel != null">#{placeToSchoolLevel},</if>
            <if test="placeToSchool != null">#{placeToSchool},</if>
            <if test="placeToLeave != null">#{placeToLeave},</if>
            <if test="jointType != null">#{jointType},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
        </trim>
    </insert>

    <update id="updateStuInfo" parameterType="StuInfo">
        update stu_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="studentId != null">student_id = #{studentId},</if>
            <if test="studentName != null and studentName != ''">student_name = #{studentName},</if>
            <if test="idNum != null">id_num = #{idNum},</if>
            <if test="deptId != null">dept_id = #{deptId},</if>
            <if test="deptName != null">dept_name = #{deptName},</if>
            <if test="trainingLevel != null">training_level = #{trainingLevel},</if>
            <if test="grade != null">grade = #{grade},</if>
            <if test="address != null">address = #{address},</if>
            <if test="nation != null">nation = #{nation},</if>
            <if test="campus != null">campus = #{campus},</if>
            <if test="studentTag != null">student_tag = #{studentTag},</if>
            <if test="accommodation != null">accommodation = #{accommodation},</if>
            <if test="accommodationPark != null">accommodation_park = #{accommodationPark},</if>
            <if test="dormitoryNo != null">dormitory_no = #{dormitoryNo},</if>
            <if test="isOnSchool != null">is_on_school = #{isOnSchool},</if>
            <if test="controlLevel != null">control_level = #{controlLevel},</if>
            <if test="notSchoolReason != null">not_school_reason = #{notSchoolReason},</if>
            <if test="placeToSchoolLevel != null">place_to_school_level = #{placeToSchoolLevel},</if>
            <if test="placeToSchool != null">place_to_school = #{placeToSchool},</if>
            <if test="placeToLeave != null">place_to_leave = #{placeToLeave},</if>
            <if test="jointType != null">joint_type = #{jointType},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteStuInfoById" parameterType="Long">
        delete from stu_info where id = #{id}
    </delete>

    <delete id="deleteStuInfoByIds" parameterType="String">
        delete from stu_info where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>