    <sql id="selectBaoliBizOrderVo">
        select a.public_account,a.account_name,a.card_person_rate,a.store_fee_rate,a.rebate_rate,a.license_date,a.adjust_loan_amount,a.work_place_location,a.domicile_place_location,a.gender,a.property_type,a.offer_trans_records,a.psbc_bank_vip,a.license_location_type,a.license_location,a.mortgage_date,a.submit_date,a.share_date,a.release_loan_date,a.detail_info_files,a.upload_loan_files,a.upload_archive_files,a.label,a.province_id,(select b.nick_name from sys_user b where b.user_id = a.applicant_id) applicant_name,(select b.city from bl_sys_cities b where b.cityid = a.city) city_name,(select b.name from baoli_biz_store b where b.id = a.store_id) store_name,concat((SELECT dict_label FROM sys_dict_data b where dict_type ='bl_biz_bank' and b.dict_value =(select agency from bl_sys_bank c where c.id = a.bank)),'/', (select `name` from bl_sys_bank b where b.id = a.bank)) bank_name,(select b.nick_name from sys_user b where b.user_id = a.inner_officer) inner_officer_name,concat((select b.`name` from  baoli_biz_car_brand b where b.id = a.level1_brand_id),' / ',(select b.`name` from  baoli_biz_car_brand b where b.id = a.level2_brand_id)) as brand_name,(select b.name from baoli_biz_car_series b where b.id = a.car_series_id) car_series_name,(select b.name from baoli_biz_car_model b where b.id = a.car_model_id) car_model_name,id, guide_price,status,refuse_order_id,applicant_id,order_schema, city, store_level, store_id, car_type, loan_type, order_number, bank, inner_officer, customer_name, apply_number, new_energy, invoice_price, loan_amount, down_payment, loan_ratio, period_number, supply_material_type, customer_phone_number, customer_work_union, is_discount, fee_ratio, is_hxr, business_number, credit_card_number, customer_age, married, exists_drive_license, domicile_type, work_place, work_field, work_category, education, companion_relationship, companion_number, create_time, update_time, create_by, update_by,level1_brand_id,level2_brand_id,car_series_id,car_model_id from baoli_