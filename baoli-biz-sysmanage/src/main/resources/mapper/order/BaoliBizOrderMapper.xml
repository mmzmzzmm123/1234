    <select id="selectMyOrderList" parameterType="BaoliBizOrder" resultMap="BaoliBizOrderResult">
        <include refid="selectBaoliBizOrderVo"/>
        <where>
            status >= '06' and refuse_order_id is null
            <if test="applicantId != null "> and applicant_id = #{applicantId}</if>
            <if test="orderYear != null "> and DATE_FORMAT(NOW(), '%Y') = #{orderYear}</if>
        </where>
    </select>

    <select id="selectMyOrder" parameterType="BaoliBizOrder" resultType="map">
        <include refid="myOrder"/>
        <where>
            status >= '06' and refuse_order_id is null
            <if test="applicantId != null "> and applicant_id = #{applicantId}</if>
            <if test="orderYear != null "> and DATE_FORMAT(NOW(), '%Y') = #{orderYear}</if>
        </where>
    </select>

    <select id="selectMyOrderCount" parameterType="BaoliBizOrder" resultType="map">
        select status,count(1) cnt from baoli_biz_order
        <where>
            refuse_order_id is null
            <!--<if test="applicantId != null "> and ((inner_officer = #{applicantId}) and (status in ('02','08','10','11')) or applicant_id = #{applicantId})</if> -->
        </where>
        group by status
    </select>

    <select id="selectBaoliBizOrderById" parameterType="Long" resultMap="BaoliBizOrderResult">
        <include refid="selectBaoliBizOrderVo"/>
