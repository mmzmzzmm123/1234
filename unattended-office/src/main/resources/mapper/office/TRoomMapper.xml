<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.office.mapper.TRoomMapper">
    
    <resultMap type="TRoom" id="TRoomResult">
        <result property="id"    column="id"    />
        <result property="storeId"    column="store_id"    />
        <result property="name"    column="name"    />
        <result property="wifi"    column="wifi"    />
        <result property="equipId"    column="equip_id"    />
        <result property="tableCode"    column="table_code"    />
        <result property="startTime"    column="start_time"    />
        <result property="stopTime"    column="stop_time"    />
        <result property="logo"    column="logo"    />
        <result property="status"    column="status"    />
        <result property="roomPayType"    column="room_pay_type"    />
        <result property="remark"    column="remark"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectTRoomVo">
        select id, store_id, name, wifi, equip_id, table_code, start_time, stop_time, logo, status, remark, create_by, create_time, update_by, update_time, room_pay_type from t_room
    </sql>

    <select id="selectTRoomList" parameterType="TRoom" resultMap="TRoomResult">
        <include refid="selectTRoomVo"/>
        <where>
            <if test="id != null and id != 0"> and id = #{id}</if>
            <if test="storeId != null  and storeId != ''"> and store_id = #{storeId}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="wifi != null  and wifi != ''"> and wifi = #{wifi}</if>
            <if test="equipId != null  and equipId != ''"> and equip_id = #{equipId}</if>
            <if test="tableCode != null  and tableCode != ''"> and table_code = #{tableCode}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="stopTime != null "> and stop_time = #{stopTime}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="roomPayType != null "> and status = #{room_pay_type}</if>
            <if test="createBy != null  and createBy != ''"> and create_by = #{createBy}</if>
        </where>
    </select>
    
    <select id="selectTRoomById" parameterType="Long" resultMap="TRoomResult">
        <include refid="selectTRoomVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTRoom" parameterType="TRoom" useGeneratedKeys="true" keyProperty="id">
        insert into t_room
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="storeId != null">store_id,</if>
            <if test="name != null and name != ''">name,</if>
            <if test="wifi != null">wifi,</if>
            <if test="equipId != null">equip_id,</if>
            <if test="tableCode != null">table_code,</if>
            <if test="startTime != null">start_time,</if>
            <if test="stopTime != null">stop_time,</if>
            <if test="logo != null">logo,</if>
            <if test="status != null">status,</if>
            <if test="roomPayType != null">room_pay_type,</if>
            <if test="remark != null">remark,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="storeId != null">#{storeId},</if>
            <if test="name != null and name != ''">#{name},</if>
            <if test="wifi != null">#{wifi},</if>
            <if test="equipId != null">#{equipId},</if>
            <if test="tableCode != null">#{tableCode},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="stopTime != null">#{stopTime},</if>
            <if test="logo != null">#{logo},</if>
            <if test="status != null">#{status},</if>
            <if test="roomPayType != null">room_pay_type,</if>
            <if test="remark != null">#{remark},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateTRoom" parameterType="TRoom">
        update t_room
        <trim prefix="SET" suffixOverrides=",">
            <if test="storeId != null">store_id = #{storeId},</if>
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="wifi != null">wifi = #{wifi},</if>
            <if test="equipId != null">equip_id = #{equipId},</if>
            <if test="tableCode != null">table_code = #{tableCode},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="stopTime != null">stop_time = #{stopTime},</if>
            <if test="logo != null">logo = #{logo},</if>
            <if test="status != null">status = #{status},</if>
            <if test="roomPayType != null">room_pay_type,</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="status != null">status = #{status},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTRoomById" parameterType="Long">
        delete from t_room where id = #{id}
    </delete>

    <delete id="deleteTRoomByIds" parameterType="String">
        delete from t_room where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>